#include<iostream>
using namespace std;
int main(){
    int x=0,y=0;
    cout<<"Please enter X coordinate :";
    cin>>x;
    cout<<"Please enter Y coordinate :";
    cin>>y;
    if (x==0&&y==0)
    {
        cout<<"The point is At origin !";
    }
    else if (x>=0&&y>=0)
    {
        cout<<"The point lies in 1st quadrant ! ";
    }
    else if (x<0&&y>=0)
    {
        cout<<"The point is at 2nd quadrant ! ";
    }
    else if (x<0&&y<0)
    {
        cout<<"The point is at 3rd quadrant ! ";
    }
    else if (x>=0&&y<0)
    {
        cout<<"The point is at 4th quadrant ! ";
    }
    return 0;
}
