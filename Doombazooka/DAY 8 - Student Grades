#include<iostream>
using namespace std;
int main(){
	int m;
	char g;
	cout<<"Please enter the marks of the student :";
	cin>>m;
	if(m>450 && m==500){
		g='A';
	}
	else if(m>400 && m==450){
		g='B';
	}
	else if(m>350 && m==400){
		g='C';
	}
	else if(m>250 && m==350){
		g='D';
	}
	else if(m>=150 && m==250){
		g='E';
	}
	else if(m<150){
		g='F';
	}
	else{
		cout<<"BruH Please Enter a Valid value";
	}
	cout<<"The Student Got "<<g<<" Grade!";
	return 0;
}
