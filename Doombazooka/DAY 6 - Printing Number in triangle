#include<iostream>
using namespace std;
int main(){
    int i,j,k=1,n;
    cout<<"Please enter the number of the rows : ";
    cin>>n;
    for (i = 0; i < n; i++)
    {
        for (j = 0; j <= i; j++)
        {
            cout<<k;
        }
        cout<<endl;
        k++;
    }
    
    return 0;
}
