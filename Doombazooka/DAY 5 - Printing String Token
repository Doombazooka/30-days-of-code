#include<iostream>
#include<string.h>
#include<string>
using namespace std;
    int main(){
    char name[700];
    int len;
    int i,j;
    cin>>name;
    len=strlen(name);
    for(i=0;i<=len;i++){
        for (j = 0; j<=i; j++)
        {
        cout<<name[j];            
        }
        cout<<endl;
    }
    return 0;
}
