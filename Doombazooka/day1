EXPIRY
//https://www.codechef.com/problems/EXPIRY
#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t!=0){
        int num;
        cin>>num;
        int days;
        cin>>days;
        int eat;
        cin>>eat;
        int total_eaten;
        total_eaten = eat*days;
        if(total_eaten>=num)
        {
            cout<<"YES"<<endl;
        }
        else{
            cout<<"NO"<<endl;
        }
        
	    t--;    
	}
	return 0;
}
